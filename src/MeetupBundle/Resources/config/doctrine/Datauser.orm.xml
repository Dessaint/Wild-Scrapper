<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MeetupBundle\Entity\Datauser" table="datauser">
    <unique-constraints>
      <unique-constraint name="UNIQ_84742E0664B64DCC" columns="userId"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="usernom" type="string" column="userNom" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userprenom" type="string" column="userPrenom" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userville" type="string" column="userVille" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userfonction" type="string" column="userFonction" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userImage" type="string" column="user_image" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userid" type="integer" column="userId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>

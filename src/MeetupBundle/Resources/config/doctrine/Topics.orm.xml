<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MeetupBundle\Entity\Topics" table="topics">
    <unique-constraints>
      <unique-constraint name="UNIQ_91F6463989FECFA9" columns="id_groupes"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idGroupes" type="integer" column="id_groupes" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="name" type="string" column="name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="groupes" target-entity="Groupes" mapped-by="topics" fetch="LAZY"/>
  </entity>
</doctrine-mapping>

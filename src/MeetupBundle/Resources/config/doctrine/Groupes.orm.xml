<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MeetupBundle\Entity\Groupes" table="groupes">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="country" type="string" column="country" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="city" type="string" column="city" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="created" type="integer" column="created" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="lon" type="decimal" column="lon" precision="10" scale="10" nullable="true"/>
    <field name="lat" type="decimal" column="lat" precision="10" scale="10" nullable="true"/>
    <field name="members" type="integer" column="members" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="name" type="string" column="name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="date" type="integer" column="date" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-many field="topics" target-entity="Topics" inversed-by="groupes" fetch="LAZY">
      <join-table name="groupes_topics">
        <join-columns>
          <join-column name="groupes_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="topics_id" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
